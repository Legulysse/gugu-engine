<Template type="ElementSpriteGroup">
    <Components>
        <Component subImage="{topLeft}">
            <UPosition xRel="0" xAbs="0" yRel="0" yAbs="0" />
            <USize xRel="0" xAbs="6" yRel="0" yAbs="6" />
        </Component>
        <Component subImage="{top}">
            <UPosition xRel="0" xAbs="6" yRel="0" yAbs="0" />
            <USize xRel="1" xAbs="-12" yRel="0" yAbs="6" />
        </Component>
        <Component subImage="{topRight}">
            <UPosition xRel="1" xAbs="-6" yRel="0" yAbs="0" />
            <USize xRel="0" xAbs="6" yRel="0" yAbs="6" />
        </Component>
        
        <Component subImage="{left}">
            <UPosition xRel="0" xAbs="0" yRel="0" yAbs="6" />
            <USize xRel="0" xAbs="6" yRel="1" yAbs="-12" />
        </Component>
        <Component subImage="{center}">
            <UPosition xRel="0" xAbs="6" yRel="0" yAbs="6" />
            <USize xRel="1" xAbs="-12" yRel="1" yAbs="-12" />
        </Component>
        <Component subImage="{right}">
            <UPosition xRel="1" xAbs="-6" yRel="0" yAbs="6" />
            <USize xRel="0" xAbs="6" yRel="1" yAbs="-12" />
        </Component>
        
        <Component subImage="{bottomLeft}">
            <UPosition xRel="0" xAbs="0" yRel="1" yAbs="-6" />
            <USize xRel="0" xAbs="6" yRel="0" yAbs="6" />
        </Component>
        <Component subImage="{bottom}">
            <UPosition xRel="0" xAbs="6" yRel="1" yAbs="-6" />
            <USize xRel="1" xAbs="-12" yRel="0" yAbs="6" />
        </Component>
        <Component subImage="{bottomRight}">
            <UPosition xRel="1" xAbs="-6" yRel="1" yAbs="-6" />
            <USize xRel="0" xAbs="6" yRel="0" yAbs="6" />
        </Component>
    </Components>
</Template>
